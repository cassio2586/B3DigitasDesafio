    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/B3Digitas.Architecture.Web/bin/Debug/net7.0/B3Digitas.Architecture.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/B3Digitas.Architecture.Web",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/B3Digitas.CriptoGearService/bin/Debug/net7.0/B3Digitas.Architecture.ServiceCrawler.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/B3Digitas.CriptoGearService",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}